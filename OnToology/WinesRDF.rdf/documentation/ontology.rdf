     -->


    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasBody -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasBody">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body"/>
        <rdfs:comment>The body of the wine.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#isFrom -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#isFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Country"/>
        <rdfs:comment>The Country of the wine.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#madeBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#madeBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Winery"/>
        <rdfs:comment>The winery that made the wine.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasAge -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasAge">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Year in which the grapes of the wine were harvested.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#hasPrice">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>The price of wine.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#numberOfReviews -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#numberOfReviews">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Number of users that reviewed the wine.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Acidity score, defined as wine&apos;s “pucker” or tartness; it&apos;s what makes a wine refreshing and your tongue salivate and want another sip.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Albarinio -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Albarinio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is only cultivated in the Rías Baixas. It produces acid wines that are very refreshing and have a very particular aroma.</rdfs:comment>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dbpedia.org/page/Albari%C3%B1o</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Body score, defined as the richness and weight of the wine in your mouth.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#CabernetSauvignon -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#CabernetSauvignon">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is one of the best known red grapes in the world. It grows in almost all major wine-growing areas, in a diverse spectrum of climates. It became famous for its presence in Bordeaux wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#CavernetFranc -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#CavernetFranc">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is native to the French Médoc region. The crossing of the Cabernet Franc with the Sauvignon Blanc resulted in the Cabernet Sauvignon. The aromas of Cabernet Franc red wines show vegetal characteristics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Chardonnay -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Chardonnay">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It comes from Burgundy but it adapts to the most diverse climates and terrains, as long as they are not too cold. For this reason, we find it in many parts of the Spanish geography, where it is aged in barrels. It results in wines with tropical and white fruit aromas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Citric -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Citric">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity"/>
        <rdfs:comment>This acid slowly disappears as it is fermented by bacteria. It is not very abundant in the grape.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Country -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Country">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Country of origin of the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Expensive -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Expensive">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Price"/>
        <rdfs:comment>Wines whose price is higher than 100 euros.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#FermentedBeverages -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#FermentedBeverages">
        <rdfs:comment>It represents the parent class of wines. Remember that wines are a type of fermented drink.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Full -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Full">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body"/>
        <rdfs:comment>The full body is the one that covers every corner of your palate and is more viscous.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Garnacha -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Garnacha">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>Its freshness and a highly accentuated fruit expression, much to the taste of today&apos;s consumer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>It is the grape variety of the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Lactic -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Lactic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity"/>
        <rdfs:comment>Lactic acid comes from the malic acid of the grape, it is much milder than this, and it is produced during fermentation by the action of lactic acid bacteria, in the so-called malolactic fermentation (after alcoholic fermentation). Lactic acid is much more abundant in reds than in whites, since the latter usually do not undergo malolactic acid.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Light -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Light">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body"/>
        <rdfs:comment>The light body is one that feels lighter and less viscous in the mouth.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Malic -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Malic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity"/>
        <rdfs:comment>It is found in greater quantity in the must and not in the wine. It is important for the conservation and life of some wines. It disappears by the action of lactic bacteria.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Medium -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Medium">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Body"/>
        <rdfs:comment>An intermediate body between light and full.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Merlot -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Merlot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is a red grape of Bordeaux (Bordeaux) origin, from the southwest of France. It began to be incorporated at the beginning of the 19th century and after Cabernet Sauvignon, it is the second most widely planted grape in the world, as a consequence of the fact that it is perfectly acclimatized to most temperate and hot climates.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Monastrell -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Monastrell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is a grape capable of producing modern, powerful, high-quality wines with a high alcohol content and a high level of tannins, especially in hot years, which sweeten after aging in the barrel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Moscatel -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Moscatel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>The muscatel grape family is very extensive. It includes some 200 grape varieties that have been used throughout history from winemaking to the production of table grapes or raisins.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Name -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Name">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Name of the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#NumReviews -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#NumReviews">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Number of users that reviewed the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Old -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Old">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Year"/>
        <rdfs:comment>A wine is classified as Old when it is older than the year 2000.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Price -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Price">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Price in euros [€] in integer value.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Rating -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Rating">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Average rating given to the wine by the users [from 1-5].</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#RedWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#RedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety"/>
        <rdfs:comment>Red wine is made mainly from red grapes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Region -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Region">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Region of the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#RoseWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#RoseWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety"/>
        <rdfs:comment>There are three ways to produce rosé wine:
1. By contact with the skins or direct pressing
2. Due to bleeding after destemming and squeezing the grapes
3. Mix red and white grapes: this method is perhaps the least used.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#SparklingWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#SparklingWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety"/>
        <rdfs:comment>Sparkling wines, as their name indicates, contain carbon dioxide naturally, never artificially. This is achieved thanks to a second fermentation, in a closed bottle or in closed tanks.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Succinic -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Succinic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity"/>
        <rdfs:comment>During fermentation is when it makes an appearance. It does not evolve with the wine and constitutes one of the elements that provide the characteristic flavor of fermented beverages.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Tartaric -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Tartaric">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Acidity"/>
        <rdfs:comment>It communicates a certain hardness when drinking if it is very abundant. If temperatures are low during the winemaking process, it can cause the formation of crystals of potassium and calcium salts, totally innocuous, which can be found in the tanks of certain bottles and which indicate that the wine has not received a correct stabilization.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Tempranillo -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Tempranillo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Grape"/>
        <rdfs:comment>It is called like this because the grapes are collected before other varieties (earlier). It is a grape with very fruity and aromatic notes, which ages quite well in barrels.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#TopRating -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#TopRating">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Rating"/>
        <rdfs:comment>Wines whose rating is higher than 4.5.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Wine variety.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#WhiteWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#WhiteWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Variety"/>
        <rdfs:comment>White wine is made from the must of white and/or red grapes, provided that the reds do not have colored pulp.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#FermentedBeverages"/>
        <rdfs:comment>This class represents a specific wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Winery -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Winery">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Winery of the wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Year -->


    <owl:Class rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Year">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#Wine"/>
        <rdfs:comment>Year in which the grapes were harvested.</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


