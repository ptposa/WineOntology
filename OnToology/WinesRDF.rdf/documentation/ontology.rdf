     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:Wines="http://www.semanticweb.org/trias/ontologies/2023/3/Wines#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/trias/ontologies/2023/3/Wines">
        <rdfs:comment>Ontology name: Wines
Author: Jorge Pablo Trias Posa</rdfs:comment>
    </owl:Ontology>
    


